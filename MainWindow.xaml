        </Button>
        <Button x:Name="buttonSeven" Content="" HorizontalAlignment="Left" Margin="96,468,0,0" VerticalAlignment="Top" Height="112" Width="120" Background="#FFB2CDFF" Click="buttonOne_Click_1" FontSize="48" FontFamily="Bauhaus 93" FontWeight="Bold" BorderBrush="Black"/>
        <Button x:Name="buttonEight" Content="" HorizontalAlignment="Left" Margin="276,468,0,0" VerticalAlignment="Top" Height="112" Width="120" Background="#FFB2CDFF" Click="buttonOne_Click_1" FontSize="48" FontFamily="Bauhaus 93" FontWeight="Bold" BorderBrush="Black"/>
