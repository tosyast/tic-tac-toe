        <Button x:Name="buttonSix" Content="" HorizontalAlignment="Left" Margin="451,331,0,0" VerticalAlignment="Top" Height="112" Width="120" Background="#FFB2CDFF" Click="buttonOne_Click_1" FontSize="48" FontFamily="Bauhaus 93" BorderBrush="Black"/>
        <Button x:Name="buttonOne" Content="" HorizontalAlignment="Left" Margin="96,198,0,0" VerticalAlignment="Top" Height="112" Width="120" Click="buttonOne_Click_1" Background="#FFB2CDFF" BorderBrush="Black" Foreground="Black" FontSize="48" FontFamily="Bauhaus 93" Cursor="ScrollNS"/>
