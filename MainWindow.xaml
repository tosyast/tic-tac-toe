        <TextBox x:Name="Xod" HorizontalAlignment="Center" Margin="0,43,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="254" Height="34" FontFamily="Bradley Hand ITC" FontSize="24" TextAlignment="Center" IsReadOnly="True"/>
        <Border BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Left" Height="0" Margin="96,198,0,0" VerticalAlignment="Top" Width="0"/>
        
    </Grid>
